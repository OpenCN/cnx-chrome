		"run_at": "document_start"
	}, {
		"matches": ["http://*.cybernations.net/*"],
		"js": ["scripts/jquery.js", "scripts/contentscript.js"],
		"run_at": "document_end"
	}],
	"permissions": ["tabs"],
	"background_page": "background.html"
}