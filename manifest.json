{
	"name": "CNExtend",
	"version": "0.0.1.0",
	"description": "CNX!",
	"icons": {
		"16": "icons/16.png",
		"48": "icons/48.png",
		"128": "icons/128.png"
	},
	"page_action": {
		"default_icon": "icons/19.png",
		"default_title": "CNExtend",
		"popup": "popup.html"
	},
	"content_scripts": [{
		"matches": ["http://*.cybernations.net/nation_drill_display.asp?*"],
